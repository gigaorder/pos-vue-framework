  "dependencies": {
    "@vue/babel-preset-jsx": "^1.1.0",
    "@vue/composition-api": "0.3.2",
    "core-js": "2.6.9",
    "lodash": "4.17.15",
    "sass": "^1.22.12",
    "vue": "2.6.10",
    "vue-fragment": "^1.5.1",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@vue/cli-plugin-babel": "3.11.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vca-jsx": "^0.3.2",
    "babel-preset-vue": "^2.0.2",
    "jest": "^24.9.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "jest-serializer-html": "^7.0.0"
  },
  "postcss": {
    "plugins": {
