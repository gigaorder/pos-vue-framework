{
  "name": "pos-vue-framework",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npm i -g @vue/cli-service @vue/cli-plugin-babel vue-template-compiler",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
